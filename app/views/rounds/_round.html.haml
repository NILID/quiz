%tr
  %td
    = link_to_if round.finished, rus_datetime(round.created_at), result_round_path(round)
  %td= round.theme.title
  %td.text-center
    %span.text-success= round.current_answers
    \ /
    %span.text-error= round.wrong_answers
  %td
    = round.finished ? round.finished : link_to(t('actions.continue'), round)